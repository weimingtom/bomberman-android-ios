<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/SinglePlayerRelativeLayoutGlobal">
	<LinearLayout android:layout_width="fill_parent"
		android:orientation="vertical" android:layout_height="fill_parent"
		android:id="@+id/linearLayout4">
		<RelativeLayout android:layout_height="wrap_content"
			android:id="@+id/SinglePlayerRelativeLayoutMenu"
			android:layout_width="fill_parent" android:background="@drawable/tilebackground"
			android:gravity="center_vertical">
			<LinearLayout android:layout_width="wrap_content"
				android:layout_alignParentRight="true" android:id="@+id/linearLayout5"
				android:layout_height="fill_parent" android:gravity="center_vertical">
				<LinearLayout android:background="@drawable/bombpower"
					android:gravity="bottom|right" android:layout_width="wrap_content"
					android:id="@+id/SinglePlayerLinearLayoutBonus1"
					android:layout_height="wrap_content">
					<TextView android:text="1" android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/SinglePlayerTextViewBombPower"></TextView>
				</LinearLayout>
				<LinearLayout android:background="@drawable/bombnumber"
					android:gravity="bottom|right" android:layout_width="wrap_content"
					android:id="@+id/SinglePlayerLinearLayoutBonus2"
					android:layout_height="wrap_content">
					<TextView android:text="1" android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/SinglePlayerTextViewBombNumber"></TextView>
				</LinearLayout>
				<LinearLayout android:background="@drawable/playerspeed"
					android:gravity="bottom|right" android:layout_width="wrap_content"
					android:id="@+id/SinglePlayerLinearLayoutBonus3"
					android:layout_height="wrap_content">
					<TextView android:text="1" android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/SinglePlayerTextViewPlayerSpeed"></TextView>
				</LinearLayout>
				<LinearLayout android:background="@drawable/playerlife"
					android:gravity="bottom|right" android:layout_width="wrap_content"
					android:id="@+id/SinglePlayerLinearLayoutBonus4"
					android:layout_height="wrap_content">
					<TextView android:text="1" android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/SinglePlayerTextViewPlayerLife"></TextView>
				</LinearLayout>
			</LinearLayout>
			<Button android:text="@string/Menu"
				android:layout_width="wrap_content" android:id="@+id/SinglePlayerButtonMenu"
				android:layout_height="wrap_content"></Button>
			<LinearLayout android:layout_width="wrap_content"
				android:layout_centerHorizontal="true" android:id="@+id/linearLayout2"
				android:layout_height="fill_parent" android:gravity="center_vertical">
				<LinearLayout android:orientation="vertical"
					android:layout_width="wrap_content" android:id="@+id/linearLayout10"
					android:layout_height="wrap_content">
					<ImageView android:id="@+id/SinglePlayerLayoutImageViewPlayer0"
						android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
				</LinearLayout>
				<LinearLayout android:layout_width="wrap_content"
					android:id="@+id/linearLayout11" android:layout_height="wrap_content">
					<ImageView android:id="@+id/SinglePlayerLayoutImageViewPlayer1"
						android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
				</LinearLayout>
				<LinearLayout android:orientation="vertical"
					android:gravity="center_horizontal" android:layout_width="wrap_content"
					android:id="@+id/linearLayout3" android:layout_height="wrap_content">
					<TextView android:text="@string/Time"
						android:layout_height="wrap_content" android:textColor="@color/White"
						android:layout_width="wrap_content" android:id="@+id/textView1"></TextView>
					<TextView android:text="TextView" android:layout_height="wrap_content"
						android:textColor="@color/White" android:layout_width="wrap_content"
						android:id="@+id/GameTextTime"></TextView>
				</LinearLayout>
				<LinearLayout android:layout_height="wrap_content"
					android:id="@+id/linearLayout12" android:layout_width="wrap_content">
					<ImageView android:id="@+id/SinglePlayerLayoutImageViewPlayer2"
						android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
				</LinearLayout>
				<LinearLayout android:layout_width="wrap_content"
					android:id="@+id/linearLayout13" android:layout_height="wrap_content">
					<ImageView android:id="@+id/SinglePlayerLayoutImageViewPlayer3"
						android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
				</LinearLayout>
			</LinearLayout>
		</RelativeLayout>
		<LinearLayout android:orientation="horizontal"
			android:layout_height="fill_parent" android:layout_width="fill_parent"
			android:id="@+id/linearLayout1">
			<LinearLayout android:background="@color/Gray"
				android:gravity="center" android:layout_height="fill_parent"
				android:layout_width="wrap_content" android:id="@+id/SinglePlayerLinearLayoutEditorController">
				<FrameLayout android:id="@+id/SinglePlayerFrameLayoutGame"
					android:layout_height="wrap_content" android:layout_width="wrap_content"></FrameLayout>
			</LinearLayout>
			<RelativeLayout android:layout_height="wrap_content"
				android:id="@+id/SinglePlayerRelativeLayoutObjectsGallery"
				android:layout_width="fill_parent" android:background="@drawable/tilebackground">
				<FrameLayout android:layout_width="wrap_content"
					android:id="@+id/SinglePlayerFrameLayoutBombsGallery"
					android:layout_centerInParent="true" android:layout_height="wrap_content"></FrameLayout>
				<ImageButton android:layout_width="wrap_content"
					android:id="@+id/SinglePlayerButtonBomb"
					android:layout_alignParentBottom="true" android:background="@drawable/bomb_button"
					android:layout_centerInParent="true" android:layout_height="wrap_content"></ImageButton>
			</RelativeLayout>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/SinglePlayerLinearLayoutMultiTouch"></LinearLayout>
	<LinearLayout android:layout_height="fill_parent"
		android:layout_width="fill_parent" android:gravity="center_vertical|center_horizontal"
		android:background="@color/Black" android:orientation="vertical"
		android:id="@+id/SinglePlayerLinearLayoutStart">
		<ImageView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/ready"
			android:id="@+id/SinglePlayerLinearLayoutStartImage"></ImageView>
	</LinearLayout>
	<LinearLayout android:layout_height="fill_parent"
		android:layout_width="fill_parent" android:gravity="center_vertical|center_horizontal"
		android:background="@color/Black" android:orientation="vertical"
		android:id="@+id/SinglePlayerLinearLayoutEnd">
		<RelativeLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:id="@+id/relativeLayout1">
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/SinglePlayerLinearLayoutEndButtonLeave"
				android:text="@string/Quit"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_alignParentRight="true" android:layout_height="wrap_content"
				android:id="@+id/SinglePlayerLinearLayoutEndButtonRestart"
				android:text="@string/Restart"></Button>
		</RelativeLayout>
		<ImageView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/winner"
			android:id="@+id/SinglePlayerLinearLayoutEndImageView"></ImageView>
	</LinearLayout>
	<LinearLayout android:layout_height="fill_parent"
		android:layout_width="fill_parent" android:gravity="center_vertical|center_horizontal"
		android:background="@color/Black" android:id="@+id/SinglePlayerLinearLayoutMenu">
		<LinearLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:gravity="center_vertical|center_horizontal"
			android:orientation="vertical" android:id="@+id/linearLayout1">
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/SinglePlayerMenuResume"
				android:text="@string/Resume"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/SinglePlayerMenuOptions"
				android:text="@string/Options"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/SinglePlayerMenuRestart"
				android:text="@string/Restart"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/SinglePlayerMenuQuit"
				android:text="@string/Quit"></Button>
		</LinearLayout>
	</LinearLayout>


</RelativeLayout>
