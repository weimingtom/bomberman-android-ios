<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/linearLayout1" android:layout_width="fill_parent"
	android:orientation="vertical" android:gravity="center_horizontal"
	android:layout_height="fill_parent" android:background="@color/Gray">

	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/linearLayout6">
		<RelativeLayout android:id="@+id/relativeLayout3"
			android:layout_width="fill_parent" android:background="@drawable/menutoolbarbackgroundxml"
			android:layout_height="50dip">
			<Button android:layout_width="wrap_content" android:id="@+id/ProfileManagerButtonCancel"
				android:text="@string/Back" android:layout_height="wrap_content"
				android:layout_centerVertical="true"></Button>
			<TextView android:layout_width="wrap_content" android:text="@string/ProfileManager"
				android:layout_height="wrap_content" android:id="@+id/textView5"
				android:textSize="20dip" android:textColor="@color/White"
				android:layout_centerInParent="true"></TextView>
			<Button android:layout_width="wrap_content"
				android:id="@+id/ProfileManagerButtonValidate"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerInParent="true" android:text="@string/Ok"></Button>
		</RelativeLayout>
	</LinearLayout>

	<TabHost android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/ProfileManagerTabHost">
		<LinearLayout android:layout_width="fill_parent"
			android:id="@+id/linearLayout2" android:layout_height="fill_parent"
			android:orientation="vertical">
			<TabWidget android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:id="@android:id/tabs"></TabWidget>
			<FrameLayout android:layout_width="fill_parent"
				android:id="@android:id/tabcontent" android:layout_height="fill_parent">
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="fill_parent" android:gravity="center_vertical|center_horizontal"
					android:orientation="vertical" android:id="@+id/ProfilLayoutSolo">
					<TableLayout android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/tableLayout1">
						<TableRow android:layout_height="wrap_content"
							android:gravity="center_vertical" android:layout_width="wrap_content"
							android:id="@+id/tableRow1">
							<TextView android:layout_width="wrap_content"
								android:text="@string/Pseudo" android:layout_height="wrap_content"
								android:id="@+id/textView2"></TextView>
							<EditText android:layout_width="wrap_content"
								android:text="EditText" android:lines="1" android:layout_height="wrap_content"
								android:id="@+id/ProfileManagerEditTextPseudo"></EditText>
						</TableRow>
					</TableLayout>
					<RelativeLayout android:layout_width="fill_parent"
						android:layout_height="wrap_content" android:id="@+id/relativeLayout1"></RelativeLayout>
					<FrameLayout android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@+id/ProfileManagerFrameLayoutObjectsGallery"
						android:layout_gravity="center_horizontal"></FrameLayout>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="fill_parent" android:gravity="center_vertical|center_horizontal"
					android:orientation="vertical" android:id="@+id/ProfilMultiLayout">
					<LinearLayout android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/linearLayout6">
						
						<TextView android:text="@string/UserName"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:id="@+id/textView5" android:layout_marginRight="10dip"></TextView>
							
						<TextView android:text="TextView" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:id="@+id/ProfileManagerUserName"></TextView>
							
					</LinearLayout>
					<LinearLayout android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:gravity="center_horizontal"
						android:id="@+id/linearLayout5">
						<Button android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:id="@+id/ProfileManagerButtonEdit"
							android:text="@string/Edit"></Button>
						<Button android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:id="@+id/ProfileManagerButtonChange"
							android:text="@string/ChangeAccount"></Button>
					</LinearLayout>
					<CheckBox android:text="@string/Autologin"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:id="@+id/ProfileManagerCheckBoxConnection"></CheckBox>
					<CheckBox android:text="@string/RememberPassword"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:id="@+id/ProfileManagerCheckBoxPassword"></CheckBox>
				</LinearLayout>
				<LinearLayout android:layout_height="fill_parent"
					android:id="@+id/ProfilGlobalLayout" android:layout_width="fill_parent"
					android:gravity="center_vertical|center_horizontal">
					<TextView android:text="@string/Type"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:id="@+id/textView6"></TextView>
					<Spinner android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:entries="@array/Type"
						android:id="@+id/spinnerType"></Spinner>
				</LinearLayout>
			</FrameLayout>
		</LinearLayout>
	</TabHost>

</LinearLayout>

