<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:id="@+id/linearLayout1" android:orientation="vertical" android:background="@color/Gray">
	
	<RelativeLayout android:id="@+id/relativeLayout3"
			android:layout_width="fill_parent" android:background="@drawable/menutoolbarbackgroundxml" android:layout_height="50dip">
			<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/textView5" android:textSize="20dip" android:textColor="@color/White" android:layout_centerInParent="true" android:text="@string/AccountEdit"></TextView>
			<Button android:id="@+id/ChangePasswordMultiplayerButtonCancel" android:text="@string/Back" android:layout_height="wrap_content" android:layout_width="wrap_content"></Button>
			<Button android:id="@+id/ChangePasswordMultiplayerButtonValidate" android:text="@string/Ok" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_alignParentRight="true"></Button>
		</RelativeLayout>
	<LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout2" android:orientation="vertical" android:layout_height="fill_parent" android:gravity="center_vertical|center_horizontal">
		<TableLayout android:id="@+id/tableLayout1" android:layout_width="wrap_content" android:layout_height="wrap_content">
			<TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tableRow1">
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/UserName" android:id="@+id/textView1"></TextView>
				<EditText android:layout_height="50dip" android:text="EditText" android:layout_marginRight="20dip" android:layout_width="120dip" android:id="@+id/ChangePasswordMultiplayerEditTextUserName"></EditText>
			</TableRow>
			<TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tableRow2">
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/OldPass" android:id="@+id/textView2"></TextView>
				<EditText android:password="true" android:text="EditText" android:layout_height="50dip" android:layout_marginRight="20dip" android:layout_width="120dip" android:id="@+id/ChangePasswordMultiplayerEditTextOldPass"></EditText>
			</TableRow>
			<TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tableRow3">
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/NewPass" android:id="@+id/textView3"></TextView>
				<EditText android:password="true" android:text="EditText" android:layout_height="50dip" android:layout_marginRight="20dip" android:layout_width="120dip" android:id="@+id/ChangePasswordMultiplayerEditTextNewPass"></EditText>
			</TableRow>
			<TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tableRow4">
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/ConfirmPass" android:id="@+id/textView4"></TextView>
				<EditText android:password="true" android:text="EditText" android:layout_height="50dip" android:layout_marginRight="20dip" android:layout_width="120dip" android:id="@+id/ChangePasswordMultiplayerEditTextConfirm"></EditText>
			</TableRow>
		</TableLayout>
	</LinearLayout>
	
	</LinearLayout>
